   <to-view-id>/pages/asignaturalist.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>carreralist</from-outcome>
   <to-view-id>/pages/carreralist.xhtml</to-view-id>
   <redirect/>
